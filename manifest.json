{
  "manifest_version": 2,
  "name": "YouTube Watch Later",
  "version": "1.0",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content_scripts/main.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icons/icon.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "enx_gr@hotmail.com",
      "data_collection_permissions": {
        "description": "This extension does not collect any user data. It only stores video links locally on your computer."
      }
    }
  }
}